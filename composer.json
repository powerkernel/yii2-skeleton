{
    "name": "modernkernel/yii2-skeleton",
    "description": "Yii 2 Skeleton Project Template",
    "keywords": ["yii2", "framework", "skeleton", "project template"],
    "homepage": "https://modernkernel.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/modernkernel/yii2-skeleton/issues?state=open",
        "source": "https://github.com/modernkernel/yii2-skeleton"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0.6",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "yiisoft/yii2-jui": "~2.0.0",
        "yiisoft/yii2-authclient": "~2.1.0",

        "himiklab/yii2-recaptcha-widget": "*",
        "conquer/select2": "*",
        "omnilight/yii2-scheduling": "*",
        "nirvana-msu/yii2-jsonld-helper": "1.0.*@dev",
        "codemix/yii2-localeurls":"*",
        "kotchuprik/yii2-sortable-widgets": "*",

        "modernkernel/yii2-bootstrapsocial": "*",
        "modernkernel/yii2-theme-adminlte": "@dev",
        "modernkernel/yii2-jeditable": "*",
        "modernkernel/yii2-slugify": "*",
        "modernkernel/yii2-fineuploader": "*",
        "modernkernel/yii2-tinymce": "*",
        "modernkernel/yii2-photoswipe": "*",

    },
    "repositories": [

    ],
    "require-dev": {
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "scripts": {
        "post-install-cmd": "php init --env=Development --overwrite=n"
    }
}
